/* String to join the date and time components of the date format string. */
"DATE_TIME_SEPARATOR" = " at ";

/* Format to represent the relative date and time string for a date equal to today and for 1 hour ago. */
"HOUR_AGO" = "%d hour ago";

/* Format to represent the relative date and time string for a date equal to today and for 1 hour from now. */
"HOUR_FROM_NOW" = "%d hour from now";

/* Format to represent the relative date and time string for a date equal to today and for more than 1 hour ago. */
"HOURS_AGO" = "%d hours ago";

/* Format to represent the relative date and time string for a date equal to today and for more than 1 hour from now. */
"HOURS_FROM_NOW" = "%d hours from now";

/* Format to represent the relative date and time string for a date equal to today and for 1 minute ago. */
"MINUTE_AGO" = "%d minute ago";

/* Format to represent the relative date and time string for a date equal to today and for 1 minute from now. */
"MINUTE_FROM_NOW" = "%d minute from now";

/* Format to represent the relative date and time string for a date equal to today and for more than 1 minute ago. */
"MINUTES_AGO" = "%d minutes ago";

/* Format to represent the relative date and time string for a date equal to today and for more than 1 minute from now. */
"MINUTES_FROM_NOW" = "%d minutes from now";

/* String to represent the relative date and time string for now. */
"NOW" = "just now";

/* Format to represent the relative date and time string for a date equal to today and for 1 second ago. */
"SECOND_AGO" = "%d second ago";

/* Format to represent the relative date and time string for a date equal to today and for 1 second from now. */
"SECOND_FROM_NOW" = "%d second from now";

/* Format to represent the relative date and time string for a date equal to today and for more than 1 minute ago. */
"SECONDS_AGO" = "%d seconds ago";

/* Format to represent the relative date and time string for a date equal to today and for more than 1 second from now. */
"SECONDS_FROM_NOW" = "%d seconds from now";

/* String to represent relative date string for today. */
"TODAY" = "today";

/* String to represent relative date string for tomorrow. */
"TOMORROW" = "tomorrow";

/* String to represent relative date string for yesterday. */
"YESTERDAY" = "yesterday";
